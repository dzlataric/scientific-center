    <bpmn:serviceTask id="saveReviewers" name="Save reviewers" camunda:class="com.scientific.center.magazine.ReviewersService">
      <bpmn:incoming>SequenceFlow_1noiejj</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1yi5ovw</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="SequenceFlow_1uc236t" sourceRef="validateMagazineInfo" targetRef="Task_0bm8ugx" />
    <bpmn:serviceTask id="RevalidateMagazineInfo" name="Re-validate magazine info" camunda:class="com.scientific.center.magazine.UpdateMagazine">
      <bpmn:incoming>SequenceFlow_0sradgb</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_0wlc8ob</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="SequenceFlow_0wlc8ob" sourceRef="RevalidateMagazineInfo" targetRef="confirmMagazine" />
