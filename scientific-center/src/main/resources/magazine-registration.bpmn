  <bpmn:process id="magazine-registration" isExecutable="true" camunda:candidateStarterGroups="editor">
    <bpmn:startEvent id="StartEvent_1" camunda:initiator="initiator">
      <bpmn:outgoing>SequenceFlow_0lkve3r</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="SequenceFlow_0lkve3r" sourceRef="StartEvent_1" targetRef="enterMagazineData" />
    <bpmn:userTask id="enterMagazineData" name="Enter magazine data" camunda:assignee="${initiator}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="name" label="Name" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="maxlength" config="50" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="issn" label="ISSN" type="string">
            <camunda:validation>
              <camunda:constraint name="required" />
              <camunda:constraint name="minlength" config="8" />
              <camunda:constraint name="maxlength" config="9" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="selectedAreas" label="Science Areas" type="collection">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="paymentTypes" label="Payment Types" type="collection">
            <camunda:validation>
              <camunda:constraint name="required" />
            </camunda:validation>
          </camunda:formField>
        </camunda:formData>
        <camunda:taskListener class="com.scientific.center.magazine.MagazineInitializer" event="create" />
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_0lkve3r</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_0jn5fc3</bpmn:outgoing>
