      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_1yi5ovw</bpmn:incoming>
      <bpmn:incoming>SequenceFlow_0wlc8ob</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_0zsfpf0</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UpdateMagazineData" name="Update magazine data" camunda:assignee="${initiator}">
