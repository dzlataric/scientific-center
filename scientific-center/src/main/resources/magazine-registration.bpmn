    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="SequenceFlow_0zsfpf0" sourceRef="confirmMagazine" targetRef="ExclusiveGateway_1kq4hok" />
    <bpmn:sequenceFlow id="SequenceFlow_18qpmv6" name="correction process" sourceRef="ExclusiveGateway_1kq4hok" targetRef="UpdateMagazineData">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${correction}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SequenceFlow_0z8gek0" sourceRef="ExclusiveGateway_1kq4hok" targetRef="enableMagazine">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!correction}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SequenceFlow_0ylc73w" sourceRef="enableMagazine" targetRef="EndEvent_0suqeqf" />
    <bpmn:sequenceFlow id="SequenceFlow_0sradgb" sourceRef="UpdateMagazineData" targetRef="RevalidateMagazineInfo" />
